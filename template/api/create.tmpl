{{define "title"}}增加接口{{end}}
{{define "head"}}{{end}}
{{define "content"}}

<div class="container">
	<div class="page-header">
		<h1>{{template "title" .}}</h1>
	</div>

	<form method="post">
		<div class="form-group">
			<label for="url">接口地址</label>
			<div><input type="text" class="form-control" id="url" name="url" maxlength="512" placeholder="Url" value="{{.url}}" /></div>
		</div>

		<div class="form-group">
			<label for="interval_time">间隔时长(分钟)</label><div>
			<input type="text" class="form-control" id="interval_time" name="interval_time" maxlength="2" placeholder="5" value="{{.interval_time}}" />	</div>
		</div>

		<input type="submit" value="创建" class="btn btn-primary" />
	</form>

	{{template "footer" .}}
</div>

{{end}}
{{define "foot"}}{{end}}
